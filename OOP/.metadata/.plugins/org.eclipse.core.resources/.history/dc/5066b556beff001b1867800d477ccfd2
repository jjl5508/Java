package com.jonathan.dojosandninjas.models;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "dojos")

public class Dojos {

	private String name;
	

	public Dojos(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
}
